{
  "include": [
    "src",
    "types"
  ],
  "compilerOptions": {
    "lib": [
      "dom",
      "esnext"
    ],
    "target": "esnext",
    "module": "esnext",
    "esModuleInterop": true,
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "declaration": true,
    "moduleResolution": "node",
    "plugins": [{ 
      "transform": "typia/lib/transform"
    }]
  }
}
