{
  "version": "1.2.0",
  "license": "MIT",
  "name": "create-pkgbld",
  "author": "Konstantin Shutkin",
  "bin": "./dist/index.cjs",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kshutkin/package-build.git"
  },
  "bugs": {
    "url": "https://github.com/kshutkin/package-build/issues"
  },
  "homepage": "https://github.com/kshutkin/package-build/blob/main/create-pkgbld/README.md",
  "keywords": [
    "pkgbld",
    "create",
    "init"
  ],
  "scripts": {
    "build": "node ../pkgbld/dist/index.js --formats=cjs,es",
    "lint": "eslint ./src"
  },
  "devDependencies": {
    "@types/git-user-name": "2.0.1",
    "@types/parse-git-config": "3.0.1",
    "@types/prompts": "2.4.4",
    "@types/minimist": "1.2.2",
    "pkgbld": "workspace:*"
  },
  "dependencies": {
    "git-user-name": "2.0.0",
    "minimist": "1.2.8",
    "parse-git-config": "3.0.0",
    "prompts": "2.4.2"
  }
}
