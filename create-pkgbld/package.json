{
  "version": "1.7.3",
  "name": "create-pkgbld",
  "license": "MIT",
  "author": "Konstantin Shutkin",
  "bin": "./dist/index.cjs",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "dist/index.d.ts"
      ],
      "*": [
        "dist/index.d.ts",
        "dist/*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kshutkin/package-build.git",
    "directory": "create-pkgbld"
  },
  "bugs": "https://github.com/kshutkin/package-build/issues",
  "homepage": "https://github.com/kshutkin/package-build/blob/main/create-pkgbld/README.md",
  "readme": "README.md",
  "keywords": [
    "pkgbld",
    "create",
    "init"
  ],
  "scripts": {
    "build": "node ../pkgbld/dist/index.js --include-externals=options",
    "lint": "eslint ./src",
    "prepack": "node ../pkgbld/dist/index.js prune"
  },
  "dependencies": {
    "string-argv": "0.3.2",
    "git-user-name": "2.0.0",
    "cleye": "1.3.2",
    "parse-git-config": "3.0.0",
    "prompts": "2.4.2",
    "kleur": "4.1.5",
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@types/git-user-name": "2.0.3",
    "@types/parse-git-config": "3.0.3",
    "@types/prompts": "2.4.8",
    "@types/lodash": "4.14.201",
    "@total-typescript/ts-reset": "0.5.1",
    "pkgbld": "workspace:*",
    "options": "workspace:*"
  }
}
