{
  "version": "1.4.4",
  "name": "create-pkgbld",
  "license": "MIT",
  "author": "Konstantin Shutkin",
  "bin": "./dist/index.cjs",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kshutkin/package-build.git",
    "directory": "create-pkgbld"
  },
  "bugs": "https://github.com/kshutkin/package-build/issues",
  "homepage": "https://github.com/kshutkin/package-build/blob/main/create-pkgbld/README.md",
  "readme": "README.md",
  "keywords": [
    "pkgbld",
    "create",
    "init"
  ],
  "scripts": {
    "build": "node ../pkgbld/dist/index.js",
    "lint": "eslint ./src"
  },
  "dependencies": {
    "typia": "^5.0.1",
    "string-argv": "^0.3.2",
    "git-user-name": "2.0.0",
    "cleye": "^1.3.2",
    "parse-git-config": "3.0.0",
    "prompts": "2.4.2",
    "kleur": "4.1.5"
  },
  "devDependencies": {
    "@types/git-user-name": "2.0.1",
    "@types/parse-git-config": "3.0.1",
    "@types/prompts": "2.4.4",
    "@total-typescript/ts-reset": "^0.5.1",
