{
  "version": "1.7.3",
  "name": "create-pkgbld",
  "license": "MIT",
  "author": "Konstantin Shutkin",
  "bin": "./index.js",
  "main": "./dist/index.mjs",
  "type": "module",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kshutkin/package-build.git",
    "directory": "create-pkgbld"
  },
  "bugs": "https://github.com/kshutkin/package-build/issues",
  "homepage": "https://github.com/kshutkin/package-build/blob/main/create-pkgbld/README.md",
  "readme": "README.md",
  "keywords": [
    "pkgbld",
    "create",
    "init"
  ],
  "scripts": {
    "build": "pkgbld --include-externals=options --formats=es --no-exports",
    "lint": "eslint ./src",
    "prepack": "pkgbld prune --flatten"
  },
  "dependencies": {
    "string-argv": "0.3.2",
    "git-user-name": "2.0.0",
    "cleye": "1.3.2",
    "parse-git-config": "3.0.0",
    "prompts": "2.4.2",
    "kleur": "4.1.5",
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@types/git-user-name": "2.0.3",
    "@types/parse-git-config": "3.0.4",
    "@types/prompts": "2.4.9",
    "@types/lodash": "4.14.202",
    "@total-typescript/ts-reset": "0.5.1",
    "pkgbld": "workspace:*",
    "options": "workspace:*"
  }
}
